<?xml version="1.0" encoding="utf-8"?>
<CustomHealthCheckData xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Domain>hasamba.local</Domain>
	<CustomDelimiter>;!</CustomDelimiter>
    <Categories>
        <Category>
            <Id>compliance_category_id</Id>
            <Name>Windows hardening</Name>
            <Explanation>Each End point is checked against a dedicated security authority baseline according to the OS version &amp; Role</Explanation>
        </Category>
        <Category>
            <Id>zircolite_category_id</Id>
            <Name>Threat hunting</Name>
            <Explanation>Checking the Eventlog against the Sigma rules public repository &amp; custom rules created by 10Root experts</Explanation>
        </Category>
	<Category>
            <Id>yara_category_id</Id>
            <Name>Yara scanner</Name>
            <Explanation>Scanning the End-point for Yara rules matched files</Explanation>
        </Category>
	<Category>
            <Id>wesng_category_id</Id>
            <Name>Host-Based Vulnerability Scanner</Name>
            <Explanation>Authenticated host vulnerability scanner based on OS patch level &amp; MSRC DB</Explanation>
        </Category>
	<Category>
            <Id>snaffler_category_id</Id>
            <Name>Content Analyzer &amp; Classifier</Name>
            <Explanation>Analyzing file's content and classify them according to data sensitivity</Explanation>
        </Category>
    </Categories>
    <Models>
        <Model>
            <Id>compliance_model_id</Id>
            <Description>Compliance</Description>
            <Category>compliance_category_id</Category>
        </Model>
        <Model>
            <Id>zircolite_model_id</Id>
            <Description>Threat Hunting</Description>
            <Category>zircolite_category_id</Category>
        </Model>
	<Model>
            <Id>yara_model_id</Id>
            <Description>Yara scanner</Description>
            <Category>yara_category_id</Category>
        </Model>
	<Model>
            <Id>wesng_model_id</Id>
            <Description>Wesng</Description>
            <Category>wesng_category_id</Category>
        </Model>
	<Model>
            <Id>snaffler_model_id</Id>
            <Description>Snaffler</Description>
            <Category>snaffler_category_id</Category>
        </Model>
    </Models>
    <RiskRules>
        <RiskRule>
            <Id>compliance_table</Id>
			<Models>
				<Model>compliance_model_id</Model>
			</Models>
            <Maturity>2</Maturity>
            <Title>Windows Hardening</Title>
            <Description>Desc</Description>
            <TechnicalExplanation>TechExplanation</TechnicalExplanation>
	<SectionId>compliance_section_id</SectionId>
            <Solution>Solution</Solution>
	    <ReportLocation>
		<Target>panelsectioncompliance_section_id</Target>
		<Text>Windows Hardening</Text>
	    </ReportLocation>
        </RiskRule>
        <RiskRule>
            <Id>zircolite_table</Id>
			<Models>
				<Model>zircolite_model_id</Model>
			</Models>
            <Maturity>2</Maturity>
            <Title>zircolite logs</Title>
            <Description>Desc</Description>
            <TechnicalExplanation>TechExplanation</TechnicalExplanation>
<SectionId>zircolite_section_id</SectionId>
            <Solution>Solution</Solution>
	    <ReportLocation>
		<Target>panelsectionzircolite_section_id</Target>
		<Text>sigma rules</Text>
	    </ReportLocation>
        </RiskRule>
	<RiskRule>
            <Id>yara_table</Id>
			<Models>
				<Model>yara_model_id</Model>
			</Models>
            <Maturity>2</Maturity>
            <Title>Yara Scanner</Title>
            <Description>Desc</Description>
            <TechnicalExplanation>TechExplanation</TechnicalExplanation>
	<SectionId>yara_section_id</SectionId>
            <Solution>Solution</Solution>
	    <ReportLocation>
		<Target>panelsectionyara_section_id</Target>
		<Text>Yara Scanner</Text>
	    </ReportLocation>
        </RiskRule>
	<RiskRule>
            <Id>wesng_table</Id>
			<Models>
				<Model>wesng_model_id</Model>
			</Models>
            <Maturity>2</Maturity>
            <Title>Wesng</Title>
            <Description>Desc</Description>
            <TechnicalExplanation>TechExplanation</TechnicalExplanation>
	<SectionId>wesng_section_id</SectionId>
            <Solution>Solution</Solution>
	    <ReportLocation>
		<Target>panelsectionwesng_section_id</Target>
		<Text>Wesng</Text>
	    </ReportLocation>
        </RiskRule>
		<RiskRule>
            <Id>snaffler_table</Id>
			<Models>
				<Model>snaffler_model_id</Model>
			</Models>
            <Maturity>2</Maturity>
            <Title>Snaffler</Title>
            <Description>Desc</Description>
            <TechnicalExplanation>TechExplanation</TechnicalExplanation>
	<SectionId>snaffler_section_id</SectionId>
            <Solution>Solution</Solution>
	    <ReportLocation>
		<Target>panelsectionsnaffler_section_id</Target>
		<Text>Snaffler</Text>
	    </ReportLocation>
        </RiskRule>
    </RiskRules>
    <HealthcheckRiskRules>
        <!-- Risks Found In Scan-->
        <HealthcheckRiskRule>
			<Points>0</Points>
			<RiskId>compliance_table</RiskId>
			<Rationale>Windows Hardening</Rationale>
            <Details>
                <Detail>
                    <Type>SharedTable</Type>
                    <FilePath>.\Reports\windows_hardening\LastReport\Master.csv</FilePath>
                    <Id>compliance_table_id</Id>
                </Detail>
            </Details>
	</HealthcheckRiskRule>
        <HealthcheckRiskRule>
			<Points>0</Points>
			<RiskId>zircolite_table</RiskId>
			<Rationale>Sigma Rules</Rationale>
            <Details>
                <Detail>
                    <Type>SharedTable</Type>
                    <FilePath>.\Reports\zircolite\LastReport\Master.csv</FilePath>
                    <Id>zircolite_table_id</Id>
                </Detail>
            </Details>
	</HealthcheckRiskRule>
	<HealthcheckRiskRule>
			<Points>0</Points>
			<RiskId>yara_table</RiskId>
			<Rationale>Yara Scanner</Rationale>
            <Details>
                <Detail>
                    <Type>SharedTable</Type>
                    <FilePath>.\Reports\yara\lastReport\Master.csv</FilePath>
                    <Id>yara_table_id</Id>
                </Detail>
            </Details>
	</HealthcheckRiskRule>
	<HealthcheckRiskRule>
			<Points>0</Points>
			<RiskId>wesng_table</RiskId>
			<Rationale>Wesng</Rationale>
            <Details>
                <Detail>
                    <Type>SharedTable</Type>
                    <FilePath>.\Reports\wesng\lastReport\Master.csv</FilePath>
                    <Id>wesng_table_id</Id>
                </Detail>
            </Details>
	</HealthcheckRiskRule>
	<HealthcheckRiskRule>
			<Points>0</Points>
			<RiskId>snaffler_table</RiskId>
			<Rationale>Snaffler</Rationale>
            <Details>
                <Detail>
                    <Type>SharedTable</Type>
                    <FilePath>.\Reports\snaffler\lastReport\Master.csv</FilePath>
                    <Id>snaffler_table_id</Id>
                </Detail>
            </Details>
	</HealthcheckRiskRule>
    </HealthcheckRiskRules>
    <InformationSections>
        <InformationSection>
            <Id>compliance_section_id</Id>
            <Name>Windows Hardening</Name>
            <Children>
                <Child>
                    <Type>Table</Type>
                    <Id>compliance_table_id</Id>
                </Child>
            </Children>
        </InformationSection>
        <InformationSection>
            <Id>zircolite_section_id</Id>
            <Name>Sigma Rules</Name>
            <Children>
                <Child>
                    <Type>Table</Type>
                    <Id>zircolite_table_id</Id>
                </Child>
            </Children>
        </InformationSection>
	<InformationSection>
            <Id>yara_section_id</Id>
            <Name>Yara Scanner</Name>
            <Children>
                <Child>
                    <Type>Table</Type>
                    <Id>yara_table_id</Id>
                </Child>
            </Children>
        </InformationSection>
	<InformationSection>
            <Id>wesng_section_id</Id>
            <Name>Wesng</Name>
            <Children>
                <Child>
                    <Type>Table</Type>
                    <Id>wesng_table_id</Id>
                </Child>
            </Children>
        </InformationSection>
	<InformationSection>
            <Id>snaffler_section_id</Id>
            <Name>Snaffler</Name>
            <Children>
                <Child>
                    <Type>Table</Type>
                    <Id>snaffler_table_id</Id>
                </Child>
            </Children>
        </InformationSection>
    </InformationSections>
    <Tables>
        <Table>
            <Id>compliance_table_id</Id>
            <NestedTablesDirectory>.\Reports\windows_hardening\lastReport\settings</NestedTablesDirectory>
        </Table>
        <Table>
           <Id>zircolite_table_id</Id>
           <NestedTablesDirectory>.\Reports\zircolite\lastReport\settings</NestedTablesDirectory>
	   <NestedColumns>
		<NestedColumn>
			<Column>EventRecordID</Column>
			<Delimiter>,</Delimiter>
			<NestedColumnPath>.\Reports\zircolite\lastReport\eventRecords</NestedColumnPath>
		</NestedColumn>
	   </NestedColumns>
        </Table>
	  <Table>
            <Id>yara_table_id</Id>
            <NestedTablesDirectory>.\Reports\yara\lastReport\settings</NestedTablesDirectory>
        </Table>
	  <Table>
            <Id>wesng_table_id</Id>
            <NestedTablesDirectory>.\Reports\wesng\lastReport\settings</NestedTablesDirectory>
        </Table>
 	<Table>
            <Id>snaffler_table_id</Id>
            <NestedTablesDirectory>.\Reports\snaffler\lastReport\settings</NestedTablesDirectory>
	<MoreDetails>.\Reports\snaffler\lastReport\computers</MoreDetails>
        </Table>
    </Tables>   
</CustomHealthCheckData>